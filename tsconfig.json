{
    "compilerOptions": {
        "lib": ["esnext.asynciterable", "es7"],
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "outDir": "lib/src",
        "rootDir": "./src",
        "sourceMap": true
    }
}
